<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
       http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="deepLinkDateCountDao" class="cc.linkedme.dao.webapi.impl.DeepLinkDateCountDaoImpl">
        <property name="tableContainer">
            <bean class="cc.linkedme.data.dao.strategy.TableContainer">
                <property name="tablesItems">
                    <map>
                        <entry key="deepLinkDateCount" value-ref="deepLinkDateCount" />
                    </map>
                </property>
            </bean>
        </property>
    </bean>

    <bean id="deepLinkDateCount" class="cc.linkedme.data.dao.strategy.TableItem">
        <property name="itemName" value="deepLinkDateCount"/>
        <property name="dbNamePrefix"  value="count"/>
        <property name="dbCount"  value="1"/>

        <property name="tableNamePrefix" value="url_count" />
        <property name="tableCount" value="1" />
        <property name="sqls">
            <map>
                <entry key="GET_DEEPLINK_DATE_COUNT_BY_ID" value="select app_id, date, click, open, install, ios_click, ios_open, ios_install, adr_click, adr_open, adr_install, pc_click, pc_ios_scan, pc_adr_scan, pc_ios_open, pc_adr_open, pc_ios_install, pc_adr_install from $db$.$tb$ where deeplink_id = ? " />
                <entry key="GET_DEEPLINKS_DATE_COUNTS_BY_APPID" value="select deeplink_id, date, click, open, install, ios_click, ios_open, ios_install, adr_click, adr_open, adr_install, pc_click, pc_ios_scan, pc_adr_scan, pc_ios_open, pc_adr_open, pc_ios_install, pc_adr_install from $db$.$tb$ where app_id = ? " />
            </map>
        </property>
        <property name="jdbcTemplates">
            <list>
                <ref bean="deeplink_datecount_jdbctemplate_0"/>
            </list>
        </property>
    </bean>

</beans>